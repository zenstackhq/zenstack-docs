{
    "trailingSlash": false,
    "functions": {
        "api/mcp/docs.js": {
            "includeFiles": "build/mcp/**"
        }
    },
    "rewrites": [
        {
            "source": "/docs/mcp",
            "destination": "/api/mcp/docs"
        }
    ],
    "redirects": [
        {
            "source": "/docs/3.x",
            "destination": "/docs/",
            "permanent": true
        },
        {
            "source": "/docs/2.x",
            "destination": "/docs/2.x/welcome",
            "permanent": true
        }
    ]
}
